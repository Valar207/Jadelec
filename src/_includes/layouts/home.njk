            <script>
        (function () {
          var header = document.querySelector('.Home_header__ZUWxe');
          if (!header) return;
          var burger = header.querySelector('.Home_menuburger__lgVpB');
          var mobileNav = header.querySelector('.Home_nav__ZMqH2');
          if (!burger || !mobileNav) return;

          function setOpen(nextOpen) {
            header.classList.toggle('is-menu-open', nextOpen);
            burger.setAttribute('aria-expanded', String(!!nextOpen));
          }

          burger.addEventListener('click', function () {
            setOpen(!header.classList.contains('is-menu-open'));
          });

          mobileNav.addEventListener('click', function (e) {
            var target = e.target;
            if (target && target.closest && target.closest('a')) {
              setOpen(false);
            }
          });

          document.addEventListener('keydown', function (e) {
            if (e.key === 'Escape') setOpen(false);
          });

          var contactForm = document.getElementById('contactForm');
          if (contactForm) {
            contactForm.addEventListener('submit', function (e) {
              e.preventDefault();

              var status = document.getElementById('contactFormStatus');
              if (status) status.textContent = '';

              var name = document.getElementById('nom');
              var email = document.getElementById('email');
              var subject = document.getElementById('sujet');
              var message = document.getElementById('message');

              var nameValue = name ? name.value.trim() : '';
              var emailValue = email ? email.value.trim() : '';
              var subjectValue = subject ? subject.value.trim() : '';
              var messageValue = message ? message.value.trim() : '';

              var mailTo = 'contact@jadelec.net';
              var mailSubject = subjectValue || 'Demande via le site Jadelec';
              var mailBody =
                'Nom: ' + nameValue + '\n' +
                'Email: ' + emailValue + '\n\n' +
                (messageValue || '');

              var href =
                'mailto:' + encodeURIComponent(mailTo) +
                '?subject=' + encodeURIComponent(mailSubject) +
                '&body=' + encodeURIComponent(mailBody);

              var before = document.visibilityState;
              var a = document.createElement('a');
              a.href = href;
              a.style.display = 'none';
              document.body.appendChild(a);
              a.click();
              document.body.removeChild(a);

              window.setTimeout(function () {
                var after = document.visibilityState;
                if (before === 'visible' && after === 'visible' && status) {
                  status.textContent =
                    "Si rien ne s'ouvre, configure une application mail sur ton appareil (ou utilise un service de formulaire).";
                }
              }, 900);
            });
          }
        })();
      </script>
